<Page
    x:Class="DebuggingTool.Views.HelloPage"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:i="https://github.com/projektanker/icons.avalonia"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:DebuggingTool.ViewModels"
    d:DesignHeight="450"
    d:DesignWidth="800"
    NavigationBar.Header="Hello"
    mc:Ignorable="d">
    <NavigationBar.HeaderIcon>
        <i:Icon Value="mdi-account-cowboy-hat" />
    </NavigationBar.HeaderIcon>
    <StackPanel
        HorizontalAlignment="Center"
        VerticalAlignment="Center"
        Spacing="10">
        <TextBlock Text="{Binding Message}" />
        <Button Command="{Binding DBTestCommand}" Content="TestDB" />

    </StackPanel>
</Page>

